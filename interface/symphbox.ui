<interface>
  <!-- interface-name symphbox.ui -->
  <requires lib="gtk" version="4.12"/>
  <object class="GtkWindow" id="main">
    <property name="default-height">800</property>
    <property name="default-width">1200</property>
    <property name="destroy-with-parent">True</property>
    <property name="maximized">True</property>
    <property name="vexpand-set">True</property>
    <child>
      <object class="GtkPaned" id="main_container">
        <property name="margin-bottom">5</property>
        <property name="margin-end">5</property>
        <property name="margin-start">5</property>
        <property name="margin-top">5</property>
        <child>
          <object class="GtkGrid">
            <property name="column-homogeneous">True</property>
            <property name="column-spacing">5</property>
            <property name="row-spacing">5</property>
            <child>
              <object class="GtkButton" id="play_button">
                <property name="label">Play</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="stop_button">
                <property name="label">Stop</property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">1</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="step_button">
                <property name="label">Step !</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">2</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSeparator">
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">2</property>
                  <property name="row">2</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkDropDown" id="model_dropdown">
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>EulerPsi</item>
                      <item>RSW</item>
                    </items>
                  </object>
                </property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">3</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Model</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">3</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkDropDown" id="ic_dropdown">
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>Dipole</item>
                      <item>Tripole</item>
                    </items>
                  </object>
                </property>
                <layout>
                  <property name="column">1</property>
                  <property name="column-span">1</property>
                  <property name="row">4</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Initial Conditions</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">4</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkSeparator">
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">2</property>
                  <property name="row">5</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkDropDown" id="integrator_dropdown">
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>RK3</item>
                      <item>EulerForward</item>
                    </items>
                  </object>
                </property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">6</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkDropDown" id="interp_dropdown">
                <property name="model">
                  <object class="GtkStringList">
                    <items>
                      <item>Weno5</item>
                      <item>Upwind5</item>
                    </items>
                  </object>
                </property>
                <layout>
                  <property name="column">1</property>
                  <property name="row">7</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Interpolation</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">7</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="label">Integrator</property>
                <layout>
                  <property name="column">0</property>
                  <property name="column-span">1</property>
                  <property name="row">6</property>
                  <property name="row-span">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
